{
  "server": {
    "port": 7070,
    "url": "https://rdfexplorer.mint.isi.edu"
  },
  "endpoints": {
    "enabled": ["okn-mint", "domain", "mint", "export", "wings", "wikidata"],
    "domain": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://w3id.org",
      "path": "/wings/export/",
      "graph": true
    },
    "okn-mint": {
      "endpointQuery": "https://endpoint.mint.isi.edu/modelcatalog/query",
      "base": "https://w3id.org",
      "path": "/okn/i/mint",
      "graph": true
    },
    "mint": {
      "endpointQuery": "https://endpoint.mint.isi.edu/modelcatalog/query",
      "base": "https://w3id.org",
      "path": "/mint/instance",
      "graph": true
    },
    "export": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://www.opmw.org",
      "path": "/export/",
      "graph": true
    },
    "wings": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://w3id.org",
      "path": "wings"
    },
    "wikidata": {
      "endpointQuery": "https://dsbox02.isi.edu:8899/bigdata/namespace/wdq/sparql",
      "base": "https://w3id.org",
      "path": "/satellite"
    }
  }
}
